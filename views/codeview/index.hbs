{{#extend "head"}}
<link rel="stylesheet" href="/css/highlight/default.css">
<link rel="stylesheet" href="/css/highlight/vs.css">
{{/extend}}

<h3>查看代码 {{filepath}}</h3>

<p>
  <a href="/codeview/raw?path={{filepath}}" target="_blank" class="btn btn-primary">查看原始代码</a>
</p>

<p>
  <pre><code class="{{highlight_extname}}">{{filesource}}</code></pre>  
</p>


{{#extend "bodybottom"}}
<script src="/js/highlight.pack.js"></script>
<script>hljs.initHighlightingOnLoad();</script>
{{/extend}}